<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="myAppSetting" type="Chapter14_1_3.CalendarOptionSection, Chapter14-1-3" />
  </configSections>

  <myAppSetting>
    <CalendarOption StringFormat="yyyy年MM月dd日(ddd)"
                    Minimum="1900/1/1"
                    Maximum="2100/12/31"
                    MondayIsFirstDay="True" />
    </myAppSetting>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>